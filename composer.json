{
  "name": "guillaume0402/todo_list",
  "description": "TyckyList - application Todo List en MVC minimal avec autoload Composer",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-pdo": "*",
    "mongodb/mongodb": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "config/env.php",
      "config/app.php",
      "config/database.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
